pipeline {
   agent any
   stages {
       stage('Build Code') {
           steps {
               sh """
               echo "Building Artifact"
               """
           }
       }
      stage('Deploy Code') {
          steps {
               sh """
               echo "Deploying Code"
               """
          }
      }
     stage('Testing Code') {
          steps {
               sh """
               echo "Testing Code"
               """
          }
      }
     stage('Delivey Code') {
          steps {
               sh """
               echo "Delivey Code"
               """
          }
      }   
   }
}
